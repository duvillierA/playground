{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "Lib",
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "node16",
    "target": "ES2022",
    "moduleResolution": "Node16",
    "declaration": true,
    "noEmit": false,
    "isolatedModules": true
  },
  "exclude": [
    "dist"
  ]
}

